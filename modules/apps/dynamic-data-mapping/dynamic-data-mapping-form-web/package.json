{
	"dependencies": {
		"classnames": "^2.2.6",
		"clay-alert": "2.17.0",
		"clay-checkbox": "2.17.0",
		"clay-icon": "2.17.0",
		"clay-management-toolbar": "2.17.0",
		"clay-modal": "2.17.0",
		"clay-radio": "2.17.0",
		"clipboard": "2.0.4",
		"metal-dom": "2.16.8",
		"metal-drag-drop": "3.3.1",
		"metal-events": "2.16.8",
		"metal-jsx": "2.16.8",
