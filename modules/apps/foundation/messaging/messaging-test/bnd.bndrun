## Uncomment for debugging
##
#-runvm: -ea, -Xdebug, "-Xrunjdwp:transport=dt_socket,server=y,suspend=y,address=8000"

-runrequires: \
	osgi.identity;filter:='(osgi.identity=com.liferay.messaging.test)',\
	osgi.identity;filter:='(osgi.identity=com.liferay.portal.jmx)',\
	osgi.identity;filter:='(osgi.identity=org.apache.felix.gogo.command)',\
	osgi.identity;filter:='(osgi.identity=org.apache.felix.gogo.shell)',\
	osgi.identity;filter:='(osgi.identity=slf4j.log4j12)'
-resolve.effective: resolve, active
-runee: JavaSE-1.8
-runsystemcapabilities:\
	${native_capability}
-runfw: org.eclipse.osgi
-runproperties: \
	osgi.console.enable.builtin=false, \
	osgi.console=, \
	org.osgi.service.http.port=8080,\
	log4j.configuration=file:${.}/log4j.properties
-runbundles: \
	com.liferay.messaging.api;version='[1.0.0,1.0.1)',\
	com.liferay.messaging.impl;version='[1.0.0,1.0.1)',\
	com.liferay.messaging.test;version='[1.0.0,1.0.1)',\
	com.liferay.petra.concurrent;version='[1.0.0,1.0.1)',\
	com.liferay.petra.io;version='[1.0.0,1.0.1)',\
	com.liferay.petra.memory;version='[1.0.0,1.0.1)',\
	com.liferay.petra.process;version='[1.0.0,1.0.1)',\
	com.liferay.petra.reflect;version='[1.0.0,1.0.1)',\
	com.liferay.portal.jmx;version='[3.0.4,3.0.5)',\
	log4j;version='[1.2.17,1.2.18)',\
	org.apache.aries.async.promise.api;version='[1.0.1,1.0.2)',\
	org.apache.aries.spifly.dynamic.bundle;version='[1.0.8,1.0.9)',\
	org.apache.aries.util;version='[1.0.0,1.0.1)',\
	org.apache.felix.configadmin;version='[1.8.8,1.8.9)',\
	org.apache.felix.gogo.command;version='[1.0.2,1.0.3)',\
	org.apache.felix.gogo.runtime;version='[1.0.6,1.0.7)',\
	org.apache.felix.gogo.shell;version='[1.0.0,1.0.1)',\
	org.apache.felix.scr;version='[2.0.10,2.0.11)',\
	org.objectweb.asm.all.debug;version='[5.0.3,5.0.4)',\
	osgi.enroute.hamcrest.wrapper;version='[1.3.0,1.3.1)',\
	osgi.enroute.junit.wrapper;version='[4.12.0,4.12.1)',\
	slf4j.api;version='[1.7.21,1.7.22)',\
	slf4j.log4j12;version='[1.6.1,1.6.2)'
