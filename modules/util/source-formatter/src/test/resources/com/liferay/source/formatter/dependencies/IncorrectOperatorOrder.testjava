	public void testTokenTypesLt() {
		int a = 3;

		if (a < a) {
			System.out.println("OK: a < a");
		}

		if (3 < 3) {
			System.out.println("OK: 3 < 3");
		}

		if (-3 < -3) {
			System.out.println("OK: -3 < -3");
		}

		if (+3 < +3) {
