		if (a > a) {
			System.out.println("OK: a > a");
		}

		if (3 > 3) {
			System.out.println("OK: 3 > 3");
		}

		if (-3 > -3) {
			System.out.println("OK: -3 > -3");
		}

		if (+3 > +3) {
			System.out.println("OK: +3 > +3");
		}

		if (a > 3) {
			System.out.println("OK: a > 3");
		}

		if (a > +3) {
			System.out.println("OK: a > +3");
		}

		if (a > -3) {
			System.out.println("OK: a > -3");
		}

		if (3 > a) {
			System.out.println("NG: 3 > a");
		}

		if (+3 > a) {
			System.out.println("NG: +3 > a");
		}

		if (-3 > a) {
			System.out.println("NG: -3 > a");
		}
	}

	public void testTokenTypesLe() {
		int a = 3;

		if (a <= a) {
			System.out.println("OK: a <= a");
