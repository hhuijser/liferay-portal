package com.liferay.source.formatter.dependencies;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

/**
 * @author Alan Huang
 */
public class InitialCapacity {

	public int getSize() {
		List<String> list1 = new ArrayList<>();

		list1.add("1-1");

		List<String> list2 = new ArrayList<>(list1.size());

		List<String> list3 = new ArrayList<>(1);

		List<String> list4 = new ArrayList<>(_SIZE);

		Map<String, String> map = new HashMap<>(2);

		return list1.size() + list2.size() + list3.size() +
			list4.size() + _list5.size() + map.size();
	}

	private static final int _SIZE = 1;

	private final List<String> _list5 = new ArrayList<>(1);

}