KeywordBulkSelection:
    - properties:
          keywordsToAdd:
              items:
                  type: string
              type: array
          keywordsToRemove:
              items:
                  type: string
              type: array
paths:
    "/content-spaces/{content-space-id}/documents":
        get:
            parameters:
                - in: path
                  name: content-space-id
                  required: true
                  schema:
                      format: int64
                      type: integer
                - in: query
                  name: keywords
                  schema:
                      type: string
                - in: query
                  name: page
                  required: true
                  schema:
                      type: integer
                - in: query
                  name: pageSize
                  required: true
                  schema:
                      type: integer